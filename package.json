{
  "name": "location-monitor",
  "version": "1.0.0",
  "description": "High-performance, real-time, enterprise-grade location tracking and monitoring library for Node.js/NestJS",
  "author": "Mohammed Mustafa (Senior Backend Engineer)",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "location",
    "tracking",
    "realtime",
    "monitor",
    "presence",
    "geofencing",
    "gps",
    "tracking-engine",
    "nestjs",
    "nodejs",
    "fleet-management",
    "delivery-tracking",
    "driver-tracking",
    "iot",
    "websocket",
    "redis",
    "kafka",
    "rabbitmq"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=test.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "example:nodejs": "cd examples/nodejs-standalone && npm install && npm start",
    "example:nestjs": "cd examples/nest-app && npm install && npm run start:dev",
    "example:simulation": "cd examples/advanced-simulation && npm install && npm start",
    "docs:serve": "npx docsify serve docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Eng-MMustafa/location-monitor"
  },
  "bugs": {
    "url": "https://github.com/Eng-MMustafa/location-monitor/issues"
  },
  "homepage": "https://github.com/Eng-MMustafa/location-monitor#readme",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "redis": "^4.6.0",
    "kafkajs": "^2.2.4",
    "amqplib": "^0.10.3",
    "ws": "^8.16.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/jest": "^29.5.11",
    "@types/ws": "^8.5.10",
    "@types/amqplib": "^0.10.4",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "prettier": "^3.2.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
